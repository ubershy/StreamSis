<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane fx:id="root" hgap="10.0" vgap="10.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.ubershy.streamsis.gui.controllers.edit.counters.TrueCheckerCounterController">
   <columnConstraints>
      <ColumnConstraints minWidth="10.0" />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints fillHeight="false" maxHeight="1.7976931348623157E308" minHeight="25.0" prefHeight="25.0" vgrow="NEVER" />
      <RowConstraints fillHeight="false" maxHeight="1.7976931348623157E308" minHeight="25.0" prefHeight="25.0" vgrow="NEVER" />
      <RowConstraints fillHeight="false" maxHeight="1.7976931348623157E308" minHeight="25.0" prefHeight="25.0" vgrow="NEVER" />
   </rowConstraints>
   <children>
      <Label text="In this panel you can only edit the name of this Counter." wrapText="true" GridPane.vgrow="ALWAYS" />
      <Label text="You can add Checkers inside this Counter in the Structure view." wrapText="true" GridPane.rowIndex="2" />
      <Label text="This Counter counts True results of contained Checkers inside." wrapText="true" GridPane.rowIndex="1" />
   </children>
</GridPane>
